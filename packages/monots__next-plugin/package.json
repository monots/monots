{
  "name": "@monots/next-plugin",
  "version": "0.4.5",
  "description": "A Next.js plugin to make NextJS sites work with monots",
  "keywords": [],
  "repository": "https://github.com/monots/monots/tree/master/packages/monots__next-plugin",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./hook": {
      "import": "./dist/hook.esm.js",
      "require": "./dist/hook.cjs.js",
      "types": "./dist/hook.d.ts"
    },
    "./hook.js": {
      "import": "./dist/hook.esm.js",
      "require": "./dist/hook.cjs.js",
      "types": "./dist/hook.d.ts"
    },
    "./index.js": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "hook"
  ],
  "dependencies": {
    "@swc/helpers": "^0.3.16"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "webpack": "^5.72.1"
  },
  "peerDependencies": {
    "next": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "hook.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "noEmit": true,
          "skipLibCheck": true
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
