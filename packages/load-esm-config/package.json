  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/normalize-path": "^3.0.0",
    "chalk": "^5.0.1",
    "chalk-template": "^0.4.0",
    "consola": "^2.15.3",
    "debug": "^4.3.4",
    "deepmerge": "^4.2.2",
    "esbuild": "^0.14.41",
    "esbuild-plugin-alias": "^0.2.1",
    "find-up": "^6.3.0",
    "is-what": "^4.1.7",
    "normalize-path": "^3.0.0",
    "read-pkg-up": "^9.1.0",
    "tiny-invariant": "^1.2.0",
    "ts-extras": "^0.10.2",
    "tsconfck": "^2.0.1",
    "type-fest": "^2.13.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.40"
  },
  "peerDependencies": {
    "@types/node": "^16 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
