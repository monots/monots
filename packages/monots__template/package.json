{
  "name": "@monots/template",
  "version": "0.0.0",
  "description": "Templating utilities for monots.",
  "keywords": [],
  "repository": "https://github.com/monots/monots/tree/master/packages/monots__template",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@monots/types": "^0.1.9",
    "@monots/utils": "^0.5.0",
    "@types/degit": "^2.8.3",
    "@types/inquirer": "^8.2.1",
    "@types/lodash.template": "^4.5.1",
    "@types/normalize-path": "^3.0.0",
    "degit": "^2.8.4",
    "del": "^6.1.1",
    "execa": "^6.1.0",
    "inquirer": "^8.2.4",
    "is-what": "^4.1.7",
    "load-esm-config": "^0.2.0",
    "load-json-file": "^7.0.1",
    "lodash.template": "^4.5.0",
    "normalize-path": "^3.0.0",
    "recursive-copy": "^2.0.14",
    "ts-extras": "^0.10.2",
    "type-fest": "^2.13.0",
    "write-json-file": "^5.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {
      "tests": {
        "compilerOptions": {
          "baseUrl": "../src",
          "paths": {
            "@monots/template": [
              "./index.ts"
            ]
          }
        },
        "files": [
          "fixtures/base/monots.template.ts"
        ],
        "include": [
          "**/*.ts",
          "**/*.js",
          "*.ts",
          "**/monots.template.ts",
          "*.js"
        ],
        "exclude": [
          "fixtures/**",
          "!**/monots.template.ts",
          "tmp/**"
        ]
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
