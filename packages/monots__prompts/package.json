{
  "name": "@monots/prompts",
  "version": "0.0.0",
  "description": "A TypeScript port of prompts",
  "keywords": [],
  "repository": "https://github.com/monots/monots/tree/master/packages/monots__prompts",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "kleur": "^4.1.4",
    "nanoevents": "^6.0.2",
    "picocolors": "^1.0.0",
    "sisteransi": "^1.0.5",
    "type-fest": "^2.13.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.42"
  },
  "peerDependencies": {
    "@types/node": "^16 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      },
      "tests": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
