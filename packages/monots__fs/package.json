{
  "name": "@monots/fs",
  "version": "0.0.0",
  "description": "a custom file system to enable storing interop with memfs and quicker testing",
  "keywords": [],
  "repository": "https://github.com/monots/monots/tree/master/packages/monots__fs",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@monots/glob": "^0.0.0",
    "@types/diff": "^5.0.2",
    "diff": "^5.1.0",
    "memfs": "^3.4.4",
    "type-fest": "^2.13.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.42"
  },
  "peerDependencies": {
    "@types/node": "^16 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {
      "tests": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "baseUrl": ".",
          "paths": {
            "@monots/fs": [
              "../src/index.ts"
            ]
          }
        }
      },
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "noEmit": true
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
