{
  "name": "eslint-config-monots",
  "version": "0.11.0",
  "description": "Provides the monots eslint configuration",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "homepage": "https://github.com/monots/monots/tree/master/packages/eslint-config-monots",
  "repository": "https://github.com/monots/monots/tree/master/packages/eslint-config-monots",
  "license": "MIT",
  "contributors": [
    {
      "name": "Ifiok Jr.",
      "email": "ifiokotung@gmail.com",
      "url": "https://ifiokjr.com"
    }
  ],
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js"
    },
    "./full": {
      "require": "./dist/full.cjs.js"
    },
    "./full-off": {
      "require": "./dist/full-off.cjs.js"
    },
    "./full-off.js": {
      "require": "./dist/full-off.cjs.js"
    },
    "./full.js": {
      "require": "./dist/full.cjs.js"
    },
    "./index.js": {
      "require": "./dist/index.cjs.js"
    },
    "./jest": {
      "require": "./dist/jest.cjs.js"
    },
    "./jest-dom": {
      "require": "./dist/jest-dom.cjs.js"
    },
    "./jest-dom.js": {
      "require": "./dist/jest-dom.cjs.js"
    },
    "./jest.js": {
      "require": "./dist/jest.cjs.js"
    },
    "./markdown": {
      "require": "./dist/markdown.cjs.js"
    },
    "./markdown.js": {
      "require": "./dist/markdown.cjs.js"
    },
    "./package.json": "./package.json",
    "./react": {
      "require": "./dist/react.cjs.js"
    },
    "./react.js": {
      "require": "./dist/react.cjs.js"
    },
    "./svelte": {
      "require": "./dist/svelte.cjs.js"
    },
    "./svelte.js": {
      "require": "./dist/svelte.cjs.js"
    },
    "./testing-library": {
      "require": "./dist/testing-library.cjs.js"
    },
    "./testing-library.js": {
      "require": "./dist/testing-library.cjs.js"
    }
  },
  "main": "./dist/index.cjs.js",
  "files": [
    "dist",
    "full",
    "full-off",
    "jest",
    "jest-dom",
    "markdown",
    "react",
    "svelte",
    "testing-library"
  ],
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-file-progress": "^1.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.29",
    "eslint": "^8.14.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "svelte": "^3.47.0",
    "typescript": "4.7.0-beta"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "svelte": "^3",
    "typescript": "^4"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "full.ts",
      "markdown.ts",
      "react.ts",
      "svelte.ts",
      "jest.ts",
      "jest-dom.ts",
      "testing-library.ts",
      "full-off.ts"
    ],
    "mode": "library",
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "noEmit": true
        }
      }
    },
    "externalModules": []
  }
}
