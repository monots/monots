{
  "name": "eslint-config-monots",
  "version": "0.2.0",
  "description": "Provides the monots eslint configuration",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "homepage": "https://github.com/monots/monots/tree/master/packages/eslint-config-monots",
  "repository": "https://github.com/monots/monots/tree/master/packages/eslint-config-monots",
  "license": "MIT",
  "contributors": [
    {
      "name": "Ifiok Jr.",
      "email": "ifiokotung@gmail.com",
      "url": "https://ifiokjr.com"
    }
  ],
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js"
    },
    "./full": {
      "require": "./dist/full.cjs.js"
    },
    "./markdown": {
      "require": "./dist/markdown.cjs.js"
    },
    "./package.json": "./package.json",
    "./react": {
      "require": "./dist/react.cjs.js"
    }
  },
  "main": "./dist/index.cjs.js",
  "files": [
    "dist",
    "full",
    "markdown",
    "react"
  ],
  "dependencies": {
    "@kyleshevlin/eslint-plugin": "^1.3.0",
    "@rushstack/eslint-patch": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^36.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/node": "^16.9.1",
    "eslint": "^7.32.0"
  },
  "peerDependencies": {
    "eslint": "^7"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "full.ts",
      "markdown.ts",
      "react.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "module": "CommonJS"
        }
      }
    }
  }
}
