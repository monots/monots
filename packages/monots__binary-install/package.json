{
  "name": "@monots/binary-install",
  "version": "0.0.0",
  "description": "Install `.tar.gz` and `.zip` binary applications via npm",
  "keywords": [],
  "repository": "https://github.com/monots/monots/tree/master/packages/monots__binary-install",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/marked-terminal": "^3.1.3",
    "@types/tar": "^6.1.1",
    "chalk": "^5.0.1",
    "del": "^6.1.1",
    "execa": "^6.1.0",
    "marked-terminal": "^5.1.1",
    "node-fetch": "^3.2.5",
    "node-stream-zip": "^1.15.0",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.40"
  },
  "peerDependencies": {
    "@types/node": "^16 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
