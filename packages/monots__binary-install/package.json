{
  "name": "@monots/binary-install",
  "version": "0.0.0",
  "description": "Install `.tar.gz` and `.zip` binary applications via npm",
  "keywords": [],
  "repository": "https://github.com/monots/monots/tree/master/packages/monots__binary-install",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/follow-redirects": "^1.14.1",
    "@types/marked-terminal": "^3.1.3",
    "@types/tar": "^6.1.1",
    "adm-zip": "^0.5.9",
    "execa": "^6.1.0",
    "follow-redirects": "^1.15.1",
    "picocolors": "^1.0.0",
    "tar": "^6.1.11",
    "ts-extras": "^0.10.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.42"
  },
  "peerDependencies": {
    "@types/node": "^16 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      },
      "tests": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "baseUrl": "../src",
          "paths": {
            "@monots/binary-install": [
              "./index.ts"
            ]
          }
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
