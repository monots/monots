{
  "name": "prettier-config-monots",
  "version": "0.2.2",
  "description": "The monots prettier configuration",
  "homepage": "https://github.com/monots/monots/tree/master/packages/prettier-config-monots",
  "repository": "https://github.com/monots/monots/tree/master/packages/prettier-config-monots",
  "license": "MIT",
  "contributors": [
    {
      "name": "Ifiok Jr.",
      "email": "ifiokotung@gmail.com",
      "url": "https://ifiokjr.com"
    }
  ],
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js"
    },
    "./index.js": {
      "require": "./dist/index.cjs.js"
    },
    "./package.json": "./package.json",
    "./svelte": {
      "require": "./dist/svelte.cjs.js"
    },
    "./svelte.js": {
      "require": "./dist/svelte.cjs.js"
    }
  },
  "main": "./dist/index.cjs.js",
  "files": [
    "dist",
    "svelte"
  ],
  "dependencies": {
    "prettier-plugin-packagejson": "^2.2.18",
    "prettier-plugin-svelte": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/prettier": "^3.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "svelte.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "noEmit": true
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
