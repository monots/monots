{
  "name": "@monots/plugin-commands",
  "version": "0.0.0",
  "description": "A core plugin which adds cli command functionality to monots",
  "keywords": [],
  "repository": "https://github.com/monots/monots/tree/master/packages/monots__plugin-commands",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "@monots/core": "^0.16.0",
    "@monots/types": "^0.1.9",
    "@monots/utils": "^0.5.0",
    "chalk": "^5.0.1",
    "chalk-template": "^0.4.0",
    "ora": "^6.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {},
    "mode": "library",
    "externalModules": []
  }
}
